{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via npm",
      "skipFiles": [
        "<node_internals>/**",
        "dist",
        ".parcel-cache"
      ],
      "cwd": "${workspaceFolder}/src/javascript",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "start"
      ],
    }
  ]
}
